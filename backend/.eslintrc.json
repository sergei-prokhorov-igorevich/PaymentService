{
    "env": {
        "node": true
    },
    "extends": [
        "airbnb-base",
        "airbnb-typescript-base"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [],
    "rules": {
        "no-console": "off",
        "import/prefer-default-export": [
            "off",
            {
                "target": "single"
            }
        ],
        "max-len": [
            "error",
            {
                "code": 120
            }
        ]
    }
}